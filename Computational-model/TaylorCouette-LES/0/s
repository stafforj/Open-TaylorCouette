FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            cyclic;
    }

    outlet
    {
        type            cyclic;
    }

    innercylinder
    {
        type            zeroGradient;
    }

    outercylinder
    {
        type            zeroGradient;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

